@article{zouDislocationNucleationFacilitated2017,
  title = {Dislocation Nucleation Facilitated by Atomic Segregation},
  author = {Zou, Lianfeng and Yang, Chaoming and Lei, Yinkai and Zakharov, Dmitri and Wiezorek, J{\"o}rg M. K. and Su, Dong and Yin, Qiyue and Li, Jonathan and Liu, Zhenyu and Stach, Eric A. and Yang, Judith C. and Qi, Liang and Wang, Guofeng and Zhou, Guangwen},
  year = {2017},
  journal = {Nature Materials},
  volume = {17},
  number = {January},
  issn = {1476-1122},
  doi = {10.1038/nmat5034},
  abstract = {Surface segregation\textemdash the enrichment of one element at the surface, relative to the bulk\textemdash is ubiquitous to multi-component materials. Using the example of a Cu\textendash Au solid solution, we demonstrate that compositional variations induced by surface segregation are accompanied by misfit strain and the formation of dislocations in the subsurface region via a surface diiusion and trapping process. The resulting chemically ordered surface regions acts as an eeective barrier that inhibits subsequent dislocation annihilation at free surfaces. Using dynamic, atomic-scale resolution electron microscopy observations and theory modelling, we show that the dislocations are highly active, and we delineate the specific atomic-scale mechanisms associated with their nucleation, glide, climb, and annihilation at elevated temperatures. These observations provide mechanistic detail of how dislocations nucleate and migrate at heterointerfaces in dissimilar-material systems. A lloying is an established and versatile method to tune the properties of materials, including mechanical strength, ductility and toughness, corrosion resistance, and catalytic properties. Often, minor compositional modifications that occur locally in a material can lead to drastic changes in properties: at free surfaces these changes can affect corrosion resistance and catalytic function; at grain boundaries they influence fracture strength; at dislocations they alter plastic deformation behaviour; and at heterophase interfaces they affect adhesion and integrity 1\textendash 3 . Thus, the effects of variations in alloy composition can easily manifest themselves in the macroscopic world through the localized phenomena of segregation\textemdash that is, the enrichment of a material constituent at a free external surface or an internal interface. Because most engineered materials are alloys either by design or by the natural incorporation of impurities that remain after processing, segregation phenomena are of fundamental scientific interest and have enormous consequence across many materials technologies. Here we report that atomic segregation acts as a source for generating dislocations, a phenomenon that has not been recognized due to the difficulty of probing the fast dynamics of the local atomic configurations. The use of real-time transmission electron microscopy (TEM) allows us to both spatially and temporally resolve the transition of the coherent, dislocation-free interface between a Cu 3 Au-segregated surface and a Cu(Au) crystal substrate into a semi-coherent structure through the nucleation and subsequent migration of misfit accommodating dislocations. By combining these atomic-resolution observations with density functional theory (DFT) and molecular dynamics (MD) simulations, we have identified a mechanism by which atomic segregation facilitates the nucleation of dislocations at alloy surfaces. This observed coupling between segregation and dislocation formation has wide relevance, as the partitioning of alloying elements (or impurities) occurs in most multi-component materials under a suitable environmental bias. Thus, the phenomena reported here are of considerable practical importance for a wide range of material systems, properties, and reactions, which include metallurgy, nanostructure synthesis, mechanical properties, oxidation/reduction, and heterogeneous catalysis. The Cu\textendash Au alloy system is a model system for studying surface segregation in metal-based alloys because Cu and Au form a face-centred cubic (fcc) solid solution over a wide range of composition and temperature},
  file = {/Users/shyue/Zotero/storage/V9Y2IGB5/2017 - Zou et al. - Nature Materials.pdf}
}

@article{zouNanocrystallineHighEntropyAlloys2017,
