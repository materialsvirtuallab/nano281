  author = {Liu, Jue and Chang, Donghee and Whitfield, Pamela and Janssen, Yuri and Yu, Xiqian and Zhou, Yongning and Bai, Jianming and Ko, Jonathan and Nam, Kyung-wan and Wu, Lijun and Zhu, Yimei and Feygenson, Mikhail and Amatucci, Glenn and {Van der Ven}, Anton and Yang, Xiao-qing and Khalifah, Peter},
