@article{pecherAtomisticCharacterisationLi2010,
  title = {Atomistic Characterisation of {{Li}}+ Mobility and Conductivity in {{Li}}(7-x){{PS}}(6-x){{Ix}} Argyrodites from Molecular Dynamics Simulations, Solid-State {{NMR}}, and Impedance Spectroscopy.},
  author = {Pecher, Oliver and Kong, Shiao-Tong and Goebel, Thorsten and Nickel, Vera and Weichert, Katja and Reiner, Christof and Deiseroth, Hans-J{\"o}rg and Maier, Joachim and Haarmann, Frank and Zahn, Dirk},
