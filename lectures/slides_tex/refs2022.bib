@article{peiConvergenceElectronicBands2011,
  title = {Convergence of Electronic Bands for High Performance Bulk Thermoelectrics},
  author = {Pei, Yanzhong and Shi, Xiaoya and LaLonde, Aaron and Wang, Heng and Chen, Lidong and Snyder, G. Jeffrey},
  year = {2011},
