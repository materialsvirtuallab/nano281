@article{fuGarnetElectrolyteBased2017,
  title = {Toward Garnet Electrolyte\textendash Based {{Li}} Metal Batteries: {{An}} Ultrathin, Highly Effective, Artificial Solid-State Electrolyte/Metallic {{Li}} Interface},
  author = {Fu, Kun Kelvin and Gong, Yunhui and Liu, Boyang and Zhu, Yizhou and Xu, Shaomao and Yao, Yonggang and Luo, Wei and Wang, Chengwei and Lacey, Steven D and Dai, Jiaqi and Chen, Yanan and Mo, Yifei and Wachsman, Eric and Hu, Liangbing},
  year = {2017},
