@article{garcia-fernandezSecondprinciplesMethodMaterials2016,
  title = {Second-Principles Method for Materials Simulations Including Electron and Lattice Degrees of Freedom},
  author = {{Garc{\'i}a-Fern{\'a}ndez}, Pablo and Wojde{\l}, Jacek C. and {\'I}{\~n}iguez, Jorge and Junquera, Javier},
  year = {2016},
  month = may,
  journal = {Physical Review B},
  volume = {93},
  number = {19},
  pages = {195137},
  doi = {10.1103/PhysRevB.93.195137},
  abstract = {We present a first-principles-based (second-principles) scheme that permits large-scale materials simulations including both atomic and electronic degrees of freedom on the same footing. The method is based on a predictive quantum-mechanical theory\textemdash e.g., density functional theory\textemdash and its accuracy can be systematically improved at a very modest computational cost. Our approach is based on dividing the electron density of the system into a reference part\textemdash typically corresponding to the system's neutral, geometry-dependent ground state\textemdash and a deformation part\textemdash defined as the difference between the actual and reference densities. We then take advantage of the fact that the bulk part of the system's energy depends on the reference density alone; this part can be efficiently and accurately described by a force field, thus avoiding explicit consideration of the electrons. Then, the effects associated to the difference density can be treated perturbatively with good precision by working in a suitably chosen Wannier function basis. Further, the electronic model can be restricted to the bands of interest. All these features combined yield a very flexible and computationally very efficient scheme. Here we present the basic formulation of this approach, as well as a practical strategy to compute model parameters for realistic materials. We illustrate the accuracy and scope of the proposed method with two case studies, namely, the relative stability of various spin arrangements in NiO (featuring complex magnetic interactions in a strongly-correlated oxide) and the formation of a two-dimensional electron gas at the interface between band insulators LaAlO3 and SrTiO3 (featuring subtle electron-lattice couplings and screening effects). We conclude by discussing ways to overcome the limitations of the present approach (most notably, the assumption of a fixed bonding topology), as well as its many envisioned possibilities and future extensions.},
  file = {/Users/shyue/Zotero/storage/94WV2LQK/García-Fernández et al. - 2016 - Second-principles method for materials simulations.pdf;/Users/shyue/Zotero/storage/5IFW3BZR/PhysRevB.93.html}
}

