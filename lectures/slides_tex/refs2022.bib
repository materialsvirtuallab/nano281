@article{rangarjanAutomatedIdentificationEnergetically2013,
  title = {Automated Identification of Energetically Feasible Mechanisms of Complex Reaction Networks in Heterogeneous Catalysis: {{Application}} to Glycerol Conversion on Transition Metals},
