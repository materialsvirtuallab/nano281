@article{dasProbingPseudo1DIon2016,
  title = {Probing the Pseudo-1-{{D}} Ion Diffusion in Lithium Titanium Niobate Anode for {{Li-ion}} Battery},
  author = {Das, Suman and Dutta, Dipak and Araujo, Rafael B. and Chakraborty, Sudip and Ahuja, Rajeev and Bhattacharyya, Aninda J.},
  year = {2016},
