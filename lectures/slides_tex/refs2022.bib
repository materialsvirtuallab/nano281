@article{manthiramCrystalchemicalGuideUnderstanding2013,
  title = {A Crystal-Chemical Guide for Understanding Redox Energy Variations of the {{M2}}+/3+ Couples in Polyanion Cathodes for Lithium-Ion Batteries},
  author = {Manthiram, Arumugam and Gutierrez, Arturo and Benedek, Nicole A},
  year = {2013},
