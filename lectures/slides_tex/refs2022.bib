@article{piperElucidatingNaturePseudo2013,
  title = {Elucidating the {{Nature}} of {{Pseudo Jahn}}\textendash{{Teller Distortions}} in {{Li}} x {{MnPO}} 4 : {{Combining Density Functional Theory}} with {{Soft}} and {{Hard X-ray Spectroscopy}}},
  author = {Piper, L F J and Quackenbush, N F and Sallis, S and Scanlon, D O and Watson, G W and Nam, K.-W. and Yang, X.-Q. and Smith, K E and Omenya, F and Chernova, N A and Whittingham, M S},
  year = {2013},
