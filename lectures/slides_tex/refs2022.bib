@article{songHighlyEfficientThermally2017,
  title = {Highly {{Efficient}} and {{Thermally Stable K}} 3 {{AlF}} 6 :{{Mn}} 4+ as a {{Red Phosphor}} for {{Ultra-High-Performance Warm White Light-Emitting Diodes}}},
  author = {Song, Enhai and Wang, Jianqing and Shi, Jiahao and Deng, Tingting and Ye, Shi and Peng, Mingying and Wang, Jing and Wondraczek, Lothar and Zhang, Qinyuan},
