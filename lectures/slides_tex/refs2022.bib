@article{meutznerWayNewPossible2015,
  title = {On the {{Way}} to {{New Possible Na-Ion Conductors}}: {{The Voronoi-Dirichlet Approach}}, {{Data Mining}} and {{Symmetry Considerations}} in {{Ternary Na Oxides}}},
  author = {Meutzner, Falk and M{\"u}nchgesang, Wolfram and Kabanova, Natalya A. and Zschornak, Matthias and Leisegang, Tilmann and Blatov, Vladislav A. and Meyer, Dirk C.},
