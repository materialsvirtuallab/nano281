@article{cherepyRedemittingManganesedopedAluminum2016,
  title = {Red-Emitting Manganese-Doped Aluminum Nitride Phosphor},
  author = {Cherepy, Nerine J. and Payne, Stephen A. and Harvey, Nicholas M. and {\textbackslash}AAberg, Daniel and Seeley, Zachary M. and Holliday, Kiel S. and Tran, Ich C. and Zhou, Fei and Martinez, H. Paul and Demeyer, Jessica M. and Drobshoff, Alexander D. and Srivastava, Alok M. and Camardello, Samuel J. and Comanzo, Holly A. and Schlagel, Deborah L. and Lograsso, Thomas A.},
  year = {2016},
