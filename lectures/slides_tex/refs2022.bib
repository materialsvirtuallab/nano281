  file = {/Users/shyue/Zotero/storage/MPBNHYLZ/2010 - Chevrier et al. - Physical Review B.pdf;/Users/shyue/Zotero/storage/QIBQ73HU/2010 - Chevrier et al. - Physical Review B(2).pdf}
