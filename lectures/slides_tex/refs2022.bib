@article{greerExploringDeformationMechanisms2012,
  title = {Exploring {{Deformation Mechanisms}} in {{Nanostructured Materials}}},
  author = {Greer, Julia R. and Jang, Dongchan and Gu, X. Wendy},
  year = {2012},
  month = sep,
  journal = {JOM},
