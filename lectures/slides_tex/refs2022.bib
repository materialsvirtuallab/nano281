@article{yanHowMakeEfficient2020,
  title = {How to Make an Efficient Gas-Phase Heterogeneous {{CO2}} Hydrogenation Photocatalyst},
  author = {Yan, Tingjiang and Li, Na and Wang, Linlin and Liu, Qin and Jelle, Abdinoor and Wang, Lu and Xu, Yangfan and Liang, Yan and Dai, Ying and Huang, Baibiao and You, Jinmao and Ozin, Geoffrey A.},
