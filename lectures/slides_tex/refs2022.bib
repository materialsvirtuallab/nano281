@article{liBothCationicAnionic2018,
  title = {Both {{Cationic}} and {{Anionic Co-}}(de)Intercalation into a {{Metal-Oxide Material}}},
  author = {Li, Qi and Qiao, Yu and Guo, Shaohua and Jiang, Kezhu and Li, Qinghao and Wu, Jinpeng and Zhou, Haoshen},
