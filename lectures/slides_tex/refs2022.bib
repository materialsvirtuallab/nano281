  author = {Sk{\'u}lason, Egill and Bligaard, Thomas and Gudmundsd{\'o}ttir, Sigr{\'i}dur and Studt, Felix and Rossmeisl, Jan and {Abild-Pedersen}, Frank and Vegge, Tejs and J{\'o}nsson, Hannes and N{\o}rskov, Jens K},
