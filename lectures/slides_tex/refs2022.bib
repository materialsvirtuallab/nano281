  file = {/Users/shyue/Zotero/storage/BMNWJ9AB/Ruggenthaler et al. - 2014 - Quantum-electrodynamical density-functional theory.pdf;/Users/shyue/Zotero/storage/VA933NMC/PhysRevA.90.html}
